{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": 0,
    "no-trailing-spaces": 0,
    "indent": 0,
    "classProperties": 0,
    "operator-linebreak": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}
